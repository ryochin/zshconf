#

echo "Loading $HOME/.zsh/zshrc.bsd"

path=($path /stand /rescue)

# for FreeBSD 5.x
alias /entropy='cd /etc'

alias ls='/bin/ls -G'

alias tf='tail -0 -F'
alias top='top -I -s 1 -o cpu'

alias ps_jail='ps aux | grep "J "'
alias ipfw_ls='ipfw -at list'
alias get='fetch -aq'
alias systat='systat 1'

